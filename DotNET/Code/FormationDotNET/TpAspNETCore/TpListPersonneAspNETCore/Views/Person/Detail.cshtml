@model Person
<div class="container">
    @if (Model != null)
    {
        <div class="row m-1 justify-content-center align-items-center">
            <div class="col-4">
                Nom : @Model.LastName Prénom : @Model.FirstName
            </div>
            <div class="col-2">
                Phone : @Model.Phone Email: @Model.Email
            </div>
        </div>
    }
</div> 